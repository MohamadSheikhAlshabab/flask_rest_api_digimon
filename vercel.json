{
    "builds": [
      {
        "src": "index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.py"
      }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1"
      },
      "command": "pip install -r requirements.txt && python index.py"
    }
  }